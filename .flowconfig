[ignore]
.*/old/.*
.*/server/.*

[include]
.*/client/.*

[libs]
./interfaces/interfaces.js

[options]
module.name_mapper='.*\.css$' -> 'CSSModule'
module.name_mapper='^.*\.css$' -> 'CSSModule'
module.name_mapper='^[a-zA-Z0-9$_-]+\.css$' -> 'CSSModule'
module.name_mapper= '^[./a-zA-Z0-9$_-]+\.css$' -> 'CSSModule'
module.system=node
strip_root=true
