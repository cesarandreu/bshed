    "co-wait": "0.0.0",
    "cookies-js": "^1.2.1",
    "css-loader": "^0.9.1",
    "debug": "^2.1.3",
    "extract-text-webpack-plugin": "^0.5.0",
    "file-loader": "^0.8.1",
    "fluxible": "^0.3.0",
    "immutable": "^3.6.4",
    "is-generator-function": "^1.0.3",
    "is-promise": "^2.0.0",
