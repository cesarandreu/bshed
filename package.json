  "scripts": {
    "client:server": "webpack-dev-server --config ./config/webpack.babel.js --hot --inline",
    "client:html": "webpack --config ./config/webpack.babel.js --watch",
    "db": "./bin/database.js reset",
    "dev:client": "npm-run-all -p client:*",
    "dev:server": "npm-run-all -p server:*",
    "link": "npm-run-all -p link:*",
    "link:bshed": "link-package",
    "link:components": "link-package ./client/components components",
    "link:client": "link-package ./client client",
    "prepare": "npm-run-all -s prepare:clean prepare:folders",
    "prepare:clean": "rm -rf build",
    "prepare:folders": "mkdir -p ./build/s3/bshed_development && mkdir -p ./build/s3/bshed_test",
    "server:api": "nodemon -w server -r bshed/config/requires server/index.js",
    "server:s3": "s3rver -d ./build/s3/ -p 10001"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "babel": {
    "stage": 0
  },
