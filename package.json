{
  "name": "bshed",
  "description": "A bikeshedding app",
  "license": "Apache-2.0",
  "main": "",
  "private": true,
  "keywords": [],
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/cesarandreu/bshed.git"
  },
  "author": {
    "name": "Cesar Andreu",
    "email": "cesarandreu@gmail.com",
    "url": "https://github.com/cesarandreu"
  },
  "scripts": {
    "client:html": "BABEL_ENV=CLIENT_DEVELOPMENT webpack --watch",
    "client:server": "BABEL_ENV=CLIENT_DEVELOPMENT webpack-dev-server --hot --inline",
    "db:create": "./scripts/create-database.js",
    "db:destroy": "./scripts/destroy-database.js",
    "db:migrate": "knex migrate:latest",
    "dev": "npm-run-all -p dev:*",
    "dev:client": "npm-run-all -p client:*",
    "dev:server": "npm-run-all -p server:*",
    "dev:worker": "npm-run-all -p worker:*",
    "link": "npm-run-all -p link:*",
    "link:bshed": "link-package",
    "link:client": "link-package ./client client",
    "link:components": "link-package ./client/components components",
    "link:constants": "link-package ./lib/bshed-constants bshed-constants",
    "link:requires": "link-package ./lib/bshed-requires bshed-requires",
    "link:workers": "link-package ./workers bshed-workers",
    "prepare": "npm-run-all -s prepare:clean prepare:folders",
    "prepare:clean": "rm -rf build",
    "prepare:folders": "mkdir -p ./build/s3/bshed_development ./build/s3/bshed_test",
    "schema": "./scripts/update-schema.js",
    "server:api": "nodemon -w server -w config -w workers -r bshed-requires ./server",
    "server:s3": "s3rver -d ./build/s3/ -p 10001",
    "server:schema": "onchange 'server/**/*.js' -- npm run schema",
    "worker:image-updates": "./bin/image-updates-worker.js",
    "worker:process-images": "./bin/process-images-worker.js",
    "worker:vote-updates": "./bin/vote-updates-worker.js"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "babel": {
    "env": {
      "CLIENT_DEVELOPMENT": {
        "plugins": [
          "./config/babel-relay-plugin.js",
          [
            "react-transform",
            {
              "transforms": [
                {
                  "transform": "react-transform-hmr",
                  "imports": [
                    "react"
                  ],
                  "locals": [
                    "module"
                  ]
                }
              ]
            }
          ]
        ]
      }
    },
    "presets": [
      "react",
      "es2015",
      "stage-1"
    ]
  },
  "dependencies": {
    "aws-sdk": "^2.2.32",
    "babel-relay-plugin": "^0.6.3",
    "bcrypt": "^0.8.5",
    "bezier-easing": "^1.1.1",
    "bookshelf": "^0.9.1",
    "bull": "^0.7.0",
    "classnames": "^2.2.3",
    "co-body": "^4.0.0",
    "dataloader": "^1.1.0",
    "debug": "^2.2.0",
    "graphql": "^0.4.14",
    "graphql-relay": "^0.3.6",
    "history": "^1.17.0",
    "http-errors": "^1.3.1",
    "immutable": "^3.7.6",
    "invariant": "^2.2.0",
    "joi": "^7.2.2",
    "keymirror": "^0.1.1",
    "knex": "^0.9.0",
    "koa": "^2.0.0-alpha.3",
    "koa-compose": "^3.0.0",
    "koa-convert": "^1.2.0",
    "koa-logger": "^2.0.0",
    "koa-session": "^3.3.1",
    "lodash": "^4.0.1",
    "mime-types": "^2.1.9",
    "multer": "^1.1.0",
    "multer-s3": "^1.4.1",
    "node-uuid": "^1.4.7",
    "pg": "^4.4.3",
    "pump": "^1.0.1",
    "raf": "^3.1.0",
    "react": "^0.14.6",
    "react-dnd": "^2.0.2",
    "react-dnd-html5-backend": "^2.0.2",
    "react-dom": "^0.14.6",
    "react-height": "^2.0.3",
    "react-motion": "^0.4.1",
    "react-relay": "^0.6.1",
    "react-router": "^2.0.0-rc5",
    "react-router-relay": "^0.9.0",
    "redis": "^2.4.2",
    "resolve-path": "^1.3.0",
    "routington": "^1.0.3",
    "rx": "^4.0.7",
    "s3fs": "^2.4.3",
    "sharp": "^0.12.2",
    "whatwg-fetch": "^0.11.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.7.2",
    "link-package": "^0.1.0",
    "nodemon": "^1.8.1",
    "npm-run-all": "^1.5.0",
    "onchange": "^2.0.0",
    "postcss-loader": "^0.8.0",
    "react-addons-test-utils": "^0.14.6",
    "react-transform-catch-errors": "^1.0.1",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "s3rver": "0.0.12",
    "standard": "^5.4.1",
    "style-loader": "^0.13.0",
    "tape": "^4.4.0",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  }
}
