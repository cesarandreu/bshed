{
  "name": "bshed",
  "description": "bikeshedding app",
  "license": "Apache-2.0",
  "main": "",
  "keywords": [],
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/cesarandreu/bshed.git"
  },
  "author": {
    "name": "Cesar Andreu",
    "email": "cesarandreu@gmail.com",
    "url": "https://github.com/cesarandreu"
  },
  "engines": {
    "node": ">=0.11.14"
  },
  "scripts": {
    "clean": "rm -r public/*",
    "dev": "npm run dev:server && npm run dev:client",
    "dev:client": "node_modules/.bin/webpack-dev-server --inline --hot",
    "dev:server": "node_modules/.bin/nodemon server.js",
    "test": "npm run test:base -- test/**/*spec.js",
    "test:api": "npm run test:base -- test/api/*.spec.js",
    "test:models": "npm run test:base -- test/models/*.spec.js",
    "test:base": "NODE_ENV=test node_modules/.bin/6to5-node --blacklist=generators node_modules/.bin/_mocha",
    "test:coverage": "NODE_ENV=test node --harmony node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -u exports --require co-mocha test/server/**/*.spec.js",
    "fakes3": "fakes3 --root test/fakes3 -p 10001",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:create": "node_modules/.bin/create_database",
    "db:drop": "node_modules/.bin/drop_database",
    "db:refresh": "npm run db:drop && npm run db:create && npm run db:migrate"
  },
  "dependencies": {
    "6to5": "^2.7.1",
    "6to5-loader": "^2.0.0",
    "aws-sdk": "^2.0.30",
    "chai-json-schema": "^1.1.0",
    "co": "^4.0.0",
    "co-fs": "^1.2.0",
    "co-wait": "0.0.0",
    "css-loader": "^0.9.1",
    "debug": "^2.1.1",
    "extract-text-webpack-plugin": "^0.3.8",
    "flux-router-component": "^0.5.4",
    "fluxible": "^0.1.5",
    "fluxible-plugin-routr": "^0.3.0",
    "koa": "koajs/koa",
    "koa-better-body": "^1.0.7",
    "koa-compose": "^2.3.0",
    "koa-compress": "^1.0.8",
    "koa-csrf": "^2.1.3",
    "koa-jwt": "^0.1.7",
    "koa-logger": "^1.2.2",
    "koa-mount": "^1.3.0",
    "koa-response-time": "^1.0.2",
    "koa-router": "^3.7.0",
    "koa-send": "^1.3.1",
    "koa-session": "^3.1.0",
    "koa-static": "^1.4.8",
    "lodash": "^2.4.1",
    "node-uuid": "^1.4.2",
    "pg": "^4.1.0",
    "react": "^0.12.2",
    "s3": "^4.3.1",
    "sequelize": "^2.0.0-rc4",
    "sequelize-cli": "^1.0.2",
    "sequelize-migration-pg-extras": "^1.0.0",
    "style-loader": "^0.8.2",
    "thunkify": "^2.1.2",
    "uuid": "^2.0.1",
    "webpack": "^1.4.15"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "co-mocha": "^1.1.0",
    "co-pg": "^1.3.1",
    "co-supertest": "0.0.7",
    "cogent": "^1.0.1",
    "cookies": "^0.5.0",
    "csrf": "^2.0.3",
    "istanbul": "^0.3.5",
    "keygrip": "^1.0.1",
    "mocha": "^2.0.1",
    "mocha-istanbul": "^0.2.0",
    "nodemon": "^1.2.1",
    "should": "^4.3.1",
    "supertest": "^0.15.0",
    "supertest-as-promised": "^1.0.0",
    "webpack-dev-server": "^1.7.0"
  }
}
