{
  "name": "bshed",
  "description": "bikeshedding app",
  "license": "Apache-2.0",
  "main": "server.js",
  "private": true,
  "keywords": [],
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/cesarandreu/bshed.git"
  },
  "author": {
    "name": "Cesar Andreu",
    "email": "cesarandreu@gmail.com",
    "url": "https://github.com/cesarandreu"
  },
  "scripts": {
    "build": "npm run build:client",
    "build:client": "webpack --config webpack.build.js -c",
    "clean": "rm -rf public/assets test/fakes3/*",
    "cluster": "babel-node scripts/cluster",
    "db:create": "create_database",
    "db:drop": "drop_database",
    "db:migrate": "sequelize db:migrate",
    "db:refresh": "npm run db:drop && npm run db:create && npm run db:migrate",
    "dev": "npm run dev:server & npm run dev:client",
    "dev:server": "nodemon --exec babel-node -- server.js",
    "dev:client": "webpack-dev-server --inline -h -c --progress",
    "fakes3": "fakes3 --root test/fakes3 -p 10001",
    "imagemin": "imagemin client/assets/images/**/*",
    "server": "babel-node server.js",
    "test": "npm run test:base -- {models/,}test/**/*.test.*",
    "test:api": "npm run test:base -- test/api/*.test.js",
    "test:base": "NODE_ENV=test babel-node node_modules/.bin/_mocha",
    "test:client": "npm run test:base -- client/**/test/*.test.*",
    "test:coverage": "NODE_ENV=test babel-node node_modules/.bin/isparta cover --report text --report html node_modules/.bin/_mocha -- --reporter dot {models/,}test/**/*.test.*",
    "test:models": "npm run test:base -- models/test/*.test.js"
  },
  "dependencies": {
    "autoprefixer-loader": "^1.0.0",
    "aws-sdk": "^2.1.17",
