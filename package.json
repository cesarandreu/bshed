    "material-design-iconfont": "^0.1.7",
    "methods": "^1.1.1",
    "mkdirp": "^0.5.0",
    "node-uuid": "^1.4.2",
    "normalize.css": "^3.0.2",
    "pg": "^4.1.0",
    "pg-hstore": "^2.3.1",
    "qs": "^2.3.3",
    "react": "^0.13.1",
    "react-hot-loader": "^1.2.3",
    "react-router": "^0.13.1",
    "s3": "^4.4.0",
