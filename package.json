    "koa-static": "^1.4.9",
    "multer": "^1.0.5",
    "multer-s3": "^1.3.0",
    "node-fetch": "^1.3.2",
    "normalizr": "^1.0.0",
    "react": "^0.14.0-rc1",
    "react-dom": "^0.14.0-rc1",
    "react-pure-render": "^1.0.2",
    "react-redux": "^3.0.0",
    "react-relay": "^0.3.2",
