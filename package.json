{
  "name": "bshed",
  "description": "bikeshedding app",
  "license": "Apache-2.0",
  "main": "server.js",
  "keywords": [],
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/cesarandreu/bshed.git"
  },
  "author": {
    "name": "Cesar Andreu",
    "email": "cesarandreu@gmail.com",
    "url": "https://github.com/cesarandreu"
  },
  "engines": {
    "node": ">=0.11.15"
  },
  "scripts": {
    "base": "6to5-node --ignore='public|node_modules' --cache --experimental",
    "build:client": "webpack -p -c --progress",
    "clean": "rm -rf public/*",
    "dev": "parallelshell 'npm run dev:server' 'npm run dev:renderer' 'npm run dev:client'",
    "dev:server": "nodemon --exec npm run base -- server.js",
    "dev:renderer": "webpack --config webpack.renderer.js --progress -c -w",
    "dev:client": "webpack-dev-server --content-base public --port 2992 --inline -h -c --progress",
    "test": "npm run test:base -- test/**/*spec.js",
    "test:api": "npm run test:base -- test/api/*.spec.js",
    "test:models": "npm run test:base -- test/models/*.spec.js",
    "test:base": "NODE_ENV=test npm run base -- node_modules/.bin/_mocha",
    "fakes3": "fakes3 --root test/fakes3 -p 10001",
    "db:migrate": "sequelize db:migrate",
    "db:create": "create_database",
    "db:drop": "drop_database",
    "db:refresh": "npm run db:drop && npm run db:create && npm run db:migrate",
    "imagemin": "imagemin ./client/assets/images/* ./client/assets/images",
    "webpack": "webpack -c --progress",
    "webpack:live": "npm run webpack -- -w"
  },
  "dependencies": {
    "6to5": "^3.0.15",
    "6to5-loader": "^3.0.0",
    "autoprefixer-loader": "^1.0.0",
    "chai-json-schema": "^1.1.0",
    "aws-sdk": "^2.1.8",
    "co": "^4.2.0",
    "co-fs": "^1.2.0",
    "co-wait": "0.0.0",
    "cookies-js": "^1.1.0",
    "css-loader": "^0.9.1",
    "debug": "^2.1.1",
    "extract-text-webpack-plugin": "^0.3.8",
    "file-loader": "^0.8.1",
    "fluxible": "^0.1.5",
    "koa": "koajs/koa",
    "koa-better-body": "^1.0.7",
    "koa-compose": "^2.3.0",
    "koa-compress": "^1.0.8",
    "koa-csrf": "^2.1.3",
    "koa-jwt": "^0.1.7",
    "koa-logger": "^1.2.2",
    "koa-mount": "^1.3.0",
    "koa-response-time": "^1.0.2",
    "koa-router": "^3.7.0",
    "koa-send": "^1.3.1",
    "koa-session": "^3.1.0",
    "koa-static": "^1.4.8",
    "less": "^2.3.1",
    "less-loader": "^2.0.0",
    "lodash": "^2.4.1",
    "lodash.some": "^2.4.1",
    "material-design-iconfont": "^0.1.1",
    "material-ui": "^0.5.0",
    "methods": "^1.1.1",
    "mkdirp": "^0.5.0",
    "node-uuid": "^1.4.2",
    "parallelshell": "^1.0.3",
    "pg": "^4.1.0",
    "pg-hstore": "^2.3.1",
    "react": "^0.12.2",
    "react-hot-loader": "^1.1.3",
    "react-router": "^0.11.6",
    "react-tap-event-plugin": "^0.1.3",
    "s3": "^4.3.1",
    "sequelize": "^2.0.0-rc4",
    "sequelize-cli": "^1.0.2",
    "sequelize-migration-pg-extras": "^1.0.0",
    "serialize-javascript": "^1.0.0",
    "style-loader": "^0.8.2",
    "superagent": "^0.21.0",
    "thunkify": "^2.1.2",
    "url-loader": "^0.5.5",
    "uuid": "^2.0.1",
    "webpack": "^1.4.15",
    "webpack-dev-server": "^1.7.0"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "co-mocha": "^1.1.0",
    "co-pg": "^1.3.1",
    "co-supertest": "0.0.7",
    "cogent": "^1.0.1",
    "cookies": "^0.5.0",
    "csrf": "^2.0.3",
    "imagemin": "^3.1.0",
    "keygrip": "^1.0.1",
    "mocha": "^2.0.1",
    "nodemon": "^1.2.1",
    "should": "^4.3.1",
    "supertest": "^0.15.0",
    "supertest-as-promised": "^1.0.0"
  }
}
