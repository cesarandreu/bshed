{
  "name": "bshed",
  "description": "bikeshedding app",
  "license": "Apache-2.0",
  "main": "server.js",
  "private": true,
  "keywords": [],
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/cesarandreu/bshed.git"
  },
  "author": {
    "name": "Cesar Andreu",
    "email": "cesarandreu@gmail.com",
    "url": "https://github.com/cesarandreu"
  },
  "scripts": {
    "clean": "rm -rf build",
    "db:create": "create_database",
    "db:drop": "drop_database",
    "db:migrate": "sequelize db:migrate",
    "db:refresh": "npm run db:drop && npm run db:create && npm run db:migrate",
    "dev": "npm run dev:server & npm run dev:client",
    "dev:server:nodemon": "nodemon -e js,json -w ./build/ ./build/server.js",
    "dev:server:webpack": "webpack --config config/webpack.server.js --watch",
    "dev:server": "npm run dev:server:webpack & npm run dev:server:nodemon",
    "dev:client": "npm run dev:client-server & npm run dev:client-html",
    "dev:client-server": "webpack-dev-server --config config/webpack.dev.js --hot --inline",
    "dev:client-html": "webpack --config config/webpack.dev.js --watch",
    "link": "npm run link:client && npm run link:components",
    "link:components": "link-package ./client/components @components",
    "link:client": "link-package ./client @client",
    "postinstall": "npm run link",
    "fakes3": "fakes3 -r $(node -p \"require('os').tmpdir()\")/fakes3 -p 10001"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "babel": {
    "stage": 0
  },
  "dependencies": {
    "aws-sdk": "^2.2.4",
    "bcrypt": "^0.8.5",
    "classnames": "^2.1.3",
    "co-body": "^4.0.0",
    "debug": "^2.2.0",
    "express": "^4.13.3",
    "express-graphql": "^0.3.0",
    "graphql": "^0.4.4",
    "graphql-relay": "^0.3.2",
    "graphql-sequelize": "^0.10.2",
    "history": "^1.10.1",
    "image-size": "^0.3.5",
    "immutable": "^3.7.5",
    "keymirror": "^0.1.1",
    "koa": "^1.0.0",
    "koa-compose": "^2.3.0",
    "koa-logger": "^1.3.0",
    "koa-qs": "^2.0.0",
    "koa-response-time": "^1.0.2",
    "koa-router": "^5.1.2",
    "koa-session": "^3.3.1",
    "koa-static": "^1.4.9",
    "node-fetch": "^1.3.2",
    "normalizr": "^1.0.0",
    "pg": "^4.4.2",
    "pg-hstore": "^2.3.2",
    "react": "^0.14.0-rc1",
    "react-dom": "^0.14.0-rc1",
    "react-pure-render": "^1.0.2",
    "react-redux": "^2.1.2",
    "react-relay": "^0.3.2",
    "react-router": "^1.0.0-rc1",
    "react-router-relay": "^0.5.0",
    "redux": "^3.0.0",
    "sequelize": "^3.8.0",
    "serialize-javascript": "^1.1.2",
    "source-map-support": "^0.3.2",
    "ssacl-attribute-roles": "0.0.5",
    "whatwg-fetch": "^0.9.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.2",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.0",
    "babel-relay-plugin": "^0.2.5",
