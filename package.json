{
  "name": "bshed",
  "description": "bikeshedding app",
  "license": "Apache-2.0",
  "main": "server.js",
  "private": true,
  "keywords": [],
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/cesarandreu/bshed.git"
  },
  "author": {
    "name": "Cesar Andreu",
    "email": "cesarandreu@gmail.com",
    "url": "https://github.com/cesarandreu"
  },
  "scripts": {
    "build": "npm run build:client",
    "build:server": "webpack --config webpack.server.js",
    "build:client": "webpack --config webpack.build.js",
    "clean": "rm -rf public/assets build",
    "cluster": "babel-node scripts/cluster",
    "db:create": "create_database",
    "db:drop": "drop_database",
    "db:migrate": "sequelize db:migrate",
    "db:refresh": "npm run db:drop && npm run db:create && npm run db:migrate",
    "dev": "npm run dev:server & npm run dev:client",
    "dev:server:nodemon": "nodemon -e js,json -w ./build/ ./build/server.js",
    "dev:server:webpack": "webpack --config webpack.server.js --watch",
    "dev:server": "npm run dev:server:webpack & npm run dev:server:nodemon",
    "dev:client": "webpack-dev-server --hot --inline --progress",
    "fakes3": "fakes3 -r $(node -p \"require('os').tmpdir()\")/fakes3 -p 10001",
    "server": "babel-node server.js",
    "test": "NODE_ENV=test mocha {models,api}/test/**/*.test.js",
    "test:api": "NODE_ENV=test mocha api/test/*.test.js",
    "test:models": "NODE_ENV=test mocha models/test/*.test.js",
    "test:client:node": "npm run test:client:node:webpack && npm run test:client:node:mocha",
    "test:client:node:live": "npm run test:client:node:webpack -- -w & npm run test:client:node:mocha -- -w",
    "test:client:node:mocha": "NODE_ENV=test mocha --opts test/client.opts build/tests.js",
    "test:client:node:webpack": "NODE_ENV=test webpack client/tests.js build/tests.js --config webpack.mocha.js",
    "test:client:browser": "NODE_ENV=test karma start",
    "test:client:browser:live": "npm run test:client:browser -- --auto-watch --no-single-run"
  },
  "standard": {
    "parser": "babel-eslint"
  },
