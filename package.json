{
  "name": "bshed",
  "description": "A bikeshedding app",
  "license": "Apache-2.0",
  "main": "server.js",
  "private": true,
  "keywords": [],
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/cesarandreu/bshed.git"
  },
  "author": {
    "name": "Cesar Andreu",
    "email": "cesarandreu@gmail.com",
